{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./build",
    "rootDir": "./",
    "baseUrl": ".",
    "paths": {
      "sst": ["./node_modules/sst"]
    }
  },
  "include": ["sst.config.ts", "stacks/**/*"]
}
